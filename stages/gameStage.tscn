const sprite_numbers = [
	preload(\"res://sprites/number_large_0.png\"),
	preload(\"res://sprites/number_large_1.png\"),	
	preload(\"res://sprites/number_large_2.png\"),
	preload(\"res://sprites/number_large_3.png\"),
